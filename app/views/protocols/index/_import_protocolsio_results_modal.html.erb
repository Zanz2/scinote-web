<div class="modal" id="import-protocolsio-results-modal" role="dialog" aria-labelledby="import-results-modal-label">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"></button>
        <h4 class="modal-title" id="import-results-modal-label">
          <%= t("protocols.index.import_results.title") %>
        </h4>
      </div>
      <div class="modal-body">
        <%# type = (params[:type].to_sym == :public ? 3 : 2) %>
        <% renamed_name = nil %>
        <%# counter = 1 %>
        <%# byebug %>
        <% protocol = Protocol.find(@prot_id)  %>
        <% if protocol.name != @db_json['name'] %>
        <% renamed_name = protocol.name %>
        <% end %>
        <% unless renamed_name  %>
        <div style="display: block;">
          <div data-val="message"><%= t("protocols.index.import_results.message_ok_protocolsio")%></div>
        </div>
        <div style="display: block;">
          <div class="well well-sm well-protocols-results">
            <ul data-hold="items">
            <div style="display: block;">
              <li>
            <span class="label label-success"><%= t("protocols.index.import_results.row_success") %></span>
            <span data-val="message"><%= @db_json['name'] %></span>
              </li>
            </div>
            </ul>
          </div>
        </div>
        <% else %>
        <div style="display: block;">
          <div data-val="message"><%= t("protocols.index.import_results.message_ok_protocolsio")%></div>
        </div>
        <div style="display: block;">
          <div class="well well-sm well-protocols-results">
            <ul data-hold="items">
              <div style="display: block;">
                <li>
                  <span class="label label-warning"><%= t("protocols.index.import_results.row_renamed")%></span>
                  <span data-val="message"><%= @db_json['name']+t("protocols.index.row_renamed_html_protocolsio").html_safe + renamed_name%></span>
                </li>
              </div>
            </ul>
          </div>
        </div>
        <% end %>
      </div>

      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"><%= t("general.cancel") %></button>
      </div>
    </div>
  </div>
</div>
